{
  "version": 2,
  "builds": [
    {
      "src": "janka-backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "janka-project/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "janka-backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "janka-project/$1"
    }
  ]
}
